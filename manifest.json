{
	"manifest_version": 2,
	"name": "Wordsmith",
	"version": "1.0.0",
	"description": "Automatically finds and enters all available words on Wordsplay.net",
	"author": "Brent Klein",
	"permissions": [
		"activeTab",
		"http://fuzzylogicinc.net/boggle/*",
		"https://www.google.com/",
		"http://www.google.com/"
	],
	"browser_action": {
		"default_icon": "icon.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}